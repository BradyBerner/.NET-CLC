@model GameService

<table>
    @for (int y = 0; y < @Model.Size; y++)
    {
        <tr>
            @for (int x = 0; x < @Model.Size; x++)
            {
                <td width="30" height="30" align="center">@Html.Partial("_Cell", Model.Array[x, y])</td>
            }
        </tr>
    }
</table>